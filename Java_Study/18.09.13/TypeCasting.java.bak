class TypeCasting {
	public static void main(String[] args) {
		int a = 10;

		System.out.println("a = " + a);
		double b = a;
		System.out.println("b = " + b);
		double c = 3.14;
		System.out.println("c = " + c);
		int d = (int)c + 1;

		System.out.println("d = " + d);
		System.out.println("c = " + c);
		
	}
}
